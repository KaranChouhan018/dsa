#include <iostream> 

using namespace std;

int main() {
    int i, j; // Declare both i and j
    for (i = 0; i < 4; i++) {
        for (j = 0; j < 4; j++) {
            cout << "x";
        }
        cout << endl;
    }

    return 0;
}
(* 
****
****
****
**** *)

void nTriangle(int n) {
	for (int i = 1; i <= n; i++) {
		for (int j = 1; j <= i; j++) {
			cout << j << " ";
		}
		cout << endl;
	}
}

